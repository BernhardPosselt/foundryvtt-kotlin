            <li><img src="/systems/pf2e/icons/iconics/LemFull.webp"></li>
            <li><img src="/systems/pf2e/icons/iconics/QuinnFull.webp"></li>
            <li><img src="/systems/pf2e/icons/iconics/ValerosFull.webp"></li>
            <li><img src="/systems/pf2e/icons/iconics/SeoniFull.webp"></li>
            <li><img src="/systems/pf2e/icons/iconics/KyraFull.webp"></li>
            <li><img src="/systems/pf2e/icons/iconics/MerisielFull.webp"></li>
